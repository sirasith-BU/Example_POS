    <!-- latest jquery-->
    <script src="~/axelit/js/jquery-3.6.3.min.js"></script>

    <!-- Bootstrap js-->
    <script src="~/axelit/vendor/bootstrap/bootstrap.bundle.min.js"></script>

    <!-- Simple bar js-->
    <script src="~/axelit/vendor/simplebar/simplebar.js"></script>

    <!-- phosphor js -->
    <script src="~/axelit/vendor/phosphor/phosphor.js"></script>

    <!-- rating-->
    <script src="~/axelit/vendor/rating/jquery.barrating.min.js"></script>

    <!-- js-->
    <script src="~/axelit/js/ratings.js"></script>

    <!-- App js-->
    <script src="~/axelit/js/script.js"></script>

    <!-- Customizer js-->

    <script src="~/axelit/js/customizer.js"></script>

    <!-- calendar js -->
    <script src="~/axelit/js/calendar.js"></script>

    <!-- Data Table js-->
    <script src="~/axelit/vendor/datatable/jquery.dataTables.min.js"></script>
    <script src="~/axelit/vendor/datatable/datatable2/dataTables.buttons.min.js"></script>
    <script src="~/axelit/vendor/datatable/datatable2/jszip.min.js"></script>
    <script src="~/axelit/vendor/datatable/datatable2/pdfmake.min.js"></script>
    <script src="~/axelit/vendor/datatable/datatable2/vfs_fonts.js"></script>
    <script src="~/axelit/vendor/datatable/datatable2/buttons.html5.min.js"></script>
    <script src="~/axelit/vendor/datatable/datatable2/buttons.print.min.js"></script>

    <!-- Data Table js-->
    <script src="~/axelit/js/data_table.js"></script>



    <script src="~/axelits/vendor/notifications/toastify-js.js"></script>

    <script src="~/axelit/vendor/sweetalert/sweetalert.js"></script> 

    <script src="~/axelit/vendor/select/select2.min.js"></script>
    <script src="~/axelit/vendor/datepikar/flatpickr.js"></script>

    <script src="~/axelit/js/formvalidation.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
