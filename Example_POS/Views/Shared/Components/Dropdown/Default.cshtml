@model List<SysFlexItem>


<div class="mb-3">This is View Component</div>

<div class="row app-form">
    <label class="form-label">Basic</label>
    <select class="select-example form-select select-basic" name="state">
        @foreach(var item in @Model)
        {
            <option value=@item.FlexItemCode>@item.FlexItemName</option>
        }
    </select>
</div>